<body>

	<!--PreLoader-->
	<div class="loader">
		<div class="loader-inner">
			<div class="circle"></div>
		</div>
	</div>
	<!--PreLoader Ends-->

	<!-- header -->

	<!-- logo -->

	<!-- menu start -->
	<%- include ('partials/navigations.html') %>

		<!-- end header -->

		<style>
			#Pintar {
				background-color: #364B55;

			}

			#prueba {
				word-wrap: break-word;
			}

			.consultaBase {
				display: none;
			}
		</style>
		<!-- breadcrumb-section -->
		<div class="breadcrumb-section breadcrumb-bg">
			<div class="container">
				<div class="row">
					<div id="Grande" class="col-lg-8 offset-lg-2 text-center">
						<div class="breadcrumb-text">

							<h1>ADMIN</h1>
						</div>
					</div>
				</div>
			</div>
		</div>
		<!-- end breadcrumb section -->
		<style>
			.AddProducts {
				margin: 100px;
			}
		</style>
		<section class="AddProducts">
			<div class="Cuadro">
				<div id="form_status"></div>
				<div class="contact-form">
					<form method="post" id="fruitkha-contact" action="">
						<p>
							<input type="text" placeholder="Producto" name="producto" id="producto" required>
						<p> <input type="text" placeholder="Imagen (Link)" name="imagen" id="imagen" required>
						</p>
						</p>
						<p>
							<input type="number" placeholder="Precio" name="precio" id="precio" required>
						</p>
						<p><textarea name="description" id="description" cols="30" rows="10" placeholder="Descripcion"
								required>
					</textarea></p>
						<input type="hidden" name="token" value="FsWga4&@f6aw" />
						<p><input type="submit" value="Enviar"></p>

					</form>
				</div>
			</div>
			</div>
		</section>


		<section class="consultaBase">
			<div class="table-responsive">


				<table class="table table-striped table" bgcolor="white" height="200px" width="150px" align="center"
					border="red">
					<tr style="color:white" align="center" id="Pintar">
						<td>Nombre </td>
						<td>Tel√©fono </td>
						<td>Email </td>
						<td>Asunto </td>
						<td>Texto </td>
					</tr>
					<%consultasList.forEach(consultas=> { %>

						<tr>
							<td width="20%">
								<%= consultas.nombre %>
							</td>
							<td width=10px>
								<%= consultas.telefono %>
							</td>
							<td width=24px>
								<%= consultas.email %>
							</td>
							<td>
								<%= consultas.asunto %>
							</td>
							<td>
								<%= consultas.mensaje %>
							</td>
						</tr>
						<% })%>

				</table>
			</div>
		</section>

		<!-- < !--footer -->

		<%- include ('partials/footer.html') %>
			<!--	< !--end footer-->



			<!--	< !--jquery -->
